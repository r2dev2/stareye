{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@ota-meshi/eslint-plugin-svelte": "^0.26.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nollup": "^0.13.13",
    "peerjs": "^1.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.4.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "rollup -c -w --environment LIVERELOAD",
    "dev:nollup": "nollup -c --hot --port 5000 --content-base ./public",
    "dev:lan": "npm run dev:livereload > /dev/null & npm run start -- --dev --host 0.0.0.0",
    "dev": "npm run dev:nollup",
    "type:check": "svelte-check --tsconfig tsconfig.json",
    "type:check:watch": "svelte-check --watch --tsconfig tsconfig.json",
    "start": "sirv public",
    "format": "eslint --cache --fix src",
    "format:check": "eslint --cache src"
  }
}
